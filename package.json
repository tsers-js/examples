{
  "name": "@tsers/examples",
  "version": "2.0.0",
  "private": false,
  "description": "TSERSful examples",
  "author": "Matti Lankinen <m.lankinen@iki.fi> (https://github.com/milankinen)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsers-js/examples.git"
  },
  "keywords": [
    "example",
    "tsers",
    "rxjs",
    "rx"
  ],
  "scripts": {
    "examples": "mkdir -p dist && browserify -t babelify examples > dist/bundle.js && open index.html",
    "kefir-example": "mkdir -p dist && browserify -t babelify examples/kefir.js > dist/bundle.js && open index.html",
    "tutorial": "./tutorial/run_tutorial",
    "test": "eslint src test",
    "watch": "mkdir -p dist && watchify -v -t babelify examples/index.js -o dist/bundle.js"
  },
  "dependencies": {
    "@tsers/core": "2.0.0-alpha1",
    "@tsers/http": "2.0.0-alpha1",
    "@tsers/kefir": "latest",
    "@tsers/model": "2.0.0-alpha1",
    "@tsers/react": "latest",
    "@tsers/rx": "latest",
    "@tsers/snabbdom": "1.0.0-alpha1",
    "history": "^2.0.1",
    "routes": "^2.1.0",
    "rx": "^4.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.6.5",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "eslint": "2.2.0",
    "watchify": "^3.7.0"
  }
}
